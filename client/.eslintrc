{
  "extends": [
      "react-app",
      "react-app/jest"
  ],
  "rules": {
    "no-console": "warn"
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "jest"
  ],
  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "browser": true
  }
}